{
  "name": "ogre",
  "version": "0.7.1",
  "description": "ogr2ogr web client",
  "keywords": [
    "ogr2ogr",
    "GIS",
    "GeoJSON"
  ],
  "author": "Marc Harter <wavded@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/wavded/ogre.git"
  },
  "homepage": "http://github.com/wavded/ogre",
  "bin": {
    "ogre": "./bin/ogre.js"
  },
  "scripts": {
    "test": "tape \"test/*-test.js\"",
    "start": "node run.js",
    "docker-build": "docker build --rm -t wavded/ogre .",
    "docker-start": "docker run -t -p 3000:3000 -v `pwd`:/src -w /src wavded/ogre node run.js",
    "docker-test": "docker run -t -p 3000:3000 -v `pwd`:/src -w /src wavded/ogre npm test",
    "ci-test": "npm run docker-build && npm run docker-test > test.tap"
  },
  "main": "./index.js",
  "dependencies": {
    "body-parser": "^1.9.2",
    "connect-multiparty": "^1.0.3",
    "express": "^4.10.1",
    "jade": "^1.7.0",
    "ogr2ogr": "^0.5.0"
  },
  "devDependencies": {
    "supertest": "^0.14.0",
    "tape": "^3.0.1"
  },
  "engines": {
    "node": ">= 0.10.0"
  }
}
